{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": true
    }
  },
  "neoconf": {
    "plugins": {
      "lua_ls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "html": {
      "html.format.indentHandlebars": true,
      "html.format.indentInnerHtml": true
    },
    "pyright": {
      "python.analysis.typeCheckingMode": "off"
    }
  }
}
